#!/bin/bash
#
# generated by ./aktualisieren
if [ "$(id -u)" != "0" ]; then
  exit 1
fi

echo "you should:"
echo "$ ln -s $MYCONFIGDIR/vim/bundle ~/.vim/bundle"

FOLDER="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )/"
myinstall(){
  # $1: src path
  # $2: dest path
  # $3: perms
  # $4: owner
  # $5: group
  read -r -p "$2? [y/N] " response
  response=${response,,}
  if [[ $response =~ ^(yes|y)$ ]]; then
    install -D --mode=$3 --owner=$4 --group=$5 $1 $2
  fi
}
