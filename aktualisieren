#!/bin/bash
echo "copy"
cp -v ~/.bashrc bashrc
cp -v ~/.aliasrc aliasrc
cp -v ~/.xmonad/xmonad.hs ./
cp -v ~/.xmonad/xmobarrc ./
cp -v ~/.vimrc vimrc
cp -v ~/.xinitrc xinitrc
cp -v ~/.bash_profile bash_profile
cp -v ~/.emacs emacs
cp -v ~/.vimencrypt vimencrypt
cp -v ~/.Xresources Xresources
cp -v ~/.tmux.conf tmux.conf

mkdir -p etc
cp /etc/zathurarc etc/zathurarc

#/etc/modprobe.d/audio_power_save.conf
#/etc/sysctl.d/disable_watchdog.conf
#/etc/udev/rules.d/disable_wol.rules
#/etc/udev/rules.d/pci_pm.rules
#/etc/udev/rules.d/wifi_power_save.rules
#/etc/sysctl.d/laptop_mode.conf
#/etc/udev/rules.d/usb_power_save.rules

echo
echo "git"
echo

git add .
git commit -a -m "aktualisiert"
git push
