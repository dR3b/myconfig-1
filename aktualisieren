#!/bin/bash
echo "copy"
cp -vu ~/.bashrc bashrc
cp -vu ~/.aliasrc aliasrc
cp -vu ~/.xmonad/xmonad.hs ./
cp -vu ~/.xmonad/xmobarrc ./
cp -vu ~/.vimrc vimrc
cp -vu ~/.vimrc-neo vimrc-neo
cp -vu ~/.xinitrc xinitrc
cp -vu ~/.bash_profile bash_profile
cp -vu ~/.emacs emacs
cp -vu ~/.vimencrypt vimencrypt
cp -vu ~/.Xresources Xresources
cp -vu ~/.tmux.conf tmux.conf
cp -vu ~/bin/disp-controll ./bin/
cp -vu ~/bin/mylayouttoggle.sh ./bin/

mkdir -p etc
cp /etc/zathurarc etc/zathurarc

#/etc/modprobe.d/audio_power_save.conf
#/etc/sysctl.d/disable_watchdog.conf
#/etc/udev/rules.d/disable_wol.rules
#/etc/udev/rules.d/pci_pm.rules
#/etc/udev/rules.d/wifi_power_save.rules
#/etc/sysctl.d/laptop_mode.conf
#/etc/udev/rules.d/usb_power_save.rules

echo
echo "git"
echo

git add .
git commit -a -m "aktualisiert"
git push
